<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title">Bookings <span class="glyphicon glyphicon-question-sign" aria-hidden="true"></span></h3>
  </div>
  <div class="panel-body">
  <% if @bookings[state].empty? %>
    Your bookings seems empty.
  <% else %>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Space</th>
        <th>Date From</th>
        <th>Date Until</th>
      </tr>
    </thead>
    <tbody>
      <%= render @bookings[state] %>
    </tbody>
  </table>
    <%= will_paginate @bookings[state], renderer: BootstrapPagination::Rails %>
  <% end %>
  </div>
</div>
